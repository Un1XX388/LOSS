<?xml version="1.0" encoding="utf-8" ?>
<resources>

  <style name="Theme.Splash" parent="android:Theme">
    <!--<item name="android:actionBarStyle">@style/CustomActionBar</item>-->
    <item name="android:windowBackground">@drawable/splash</item>
    <item name="android:windowNoTitle">true</item>
    <!--<item name="android:icon">@android:color/transparent</item>-->

  </style>
  
  <style name="DefaultTheme" 
         parent="android:Theme.Holo">
    
    <item name="android:actionBarStyle">@style/CustomActionBar</item>
    
    <item name="android:windowBackground">@drawable/radialback</item>
    <!--<item name="android:windowBackground">@color/white</item>-->
    <item name="android:icon">@android:color/transparent</item>
  </style>

  <style name="ContrastTheme"
       parent="android:Theme.Holo">

    <item name="android:actionBarStyle">@style/CustomActionBar</item>

    <item name="android:windowBackground">@drawable/radialback2</item>
    <item name="android:icon">@android:color/transparent</item>
  </style>
   <style name="CustomActionBar" parent="android:style/Widget.Holo.ActionBar">
      <!--  <item name="android:background">#3A263C</item> -->
  </style>
</resources>